<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.Mini_proj" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <!-- <item name="colorPrimary">@color/my_dark_primary</item> -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorPrimary">@color/bluebgdark</item>
        <item name="android:colorSecondary">@color/ccd</item>
        <!-- Customize your light theme here. -->
        <item name="bg">@color/bluebgdark</item>
        <item name="cc">@color/ccd</item>
        <item name="tc">@color/bluebg</item>
        <item name="android:buttonTint">@color/bluebg</item>
        <item name="android:colorBackground">@color/bluebgdark</item>
        <item name="textAppearanceButton">@style/TextAppearance.AppCompat.Button.Custom.Dark</item>


        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>
<style name="TextAppearance.AppCompat.Button.Custom.Dark" parent="Widget.Material3.Button">
<item name="android:textColor">@color/bluebg</item>
<item name="android:backgroundTint">@color/bluebgdark</item>
    <item name="strokeColor">@color/bluebg</item>
<item name="fontFamily">@font/nova_mono</item>
</style>





</resources>